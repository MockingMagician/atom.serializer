{
    "name": "mocking-magician/atom-serializer",
    "type": "library",
    "description": "Lightweight and simple library for object serializing",
    "license": "CC-BY-SA-4.0",
    "homepage": "https://github.com/MockingMagician/atom.serializer",
    "authors": [
        {
            "name": "Marc MOREAU",
            "email": "moreau.marc.web@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MockingMagician\\Atom\\Serializer\\": "src/"
        }
